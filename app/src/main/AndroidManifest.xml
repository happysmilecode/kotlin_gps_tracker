<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
        xmlns:tools="http://schemas.android.com/tools">

    <application
            android:allowBackup="true"
            android:dataExtractionRules="@xml/data_extraction_rules"
            android:fullBackupContent="@xml/backup_rules"
            android:icon="@drawable/logo_of_singularity_indonesia__2023_stefanus_ayudha"
            android:label="@string/app_name"
            android:roundIcon="@drawable/logo_of_singularity_indonesia__2023_stefanus_ayudha"
            android:supportsRtl="true"
            android:theme="@style/Theme.GPSTracker"
            tools:targetApi="31">
        <activity
                android:name=".activity.MainActivity"
                android:exported="true"
                android:label="@string/app_name"
                android:theme="@style/Theme.GPSTracker">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <activity
                android:name=".activity.JavaActivityExample"
                android:exported="true" />
        <activity
